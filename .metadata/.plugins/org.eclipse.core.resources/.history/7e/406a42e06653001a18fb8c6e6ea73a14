import java.util.Scanner;
public class Principal {
	
	public static Funcion1 f1 = new Funcion1();
	Funcion2 f2 = new Funcion2();
	Funcion3 f3 = new Funcion3();
	Funcion4 f4 = new Funcion4();
	Funcion5 f5 = new Funcion5();
	Funcion6 f6 = new Funcion6();
	Funcion7 f7 = new Funcion7();
	Funcion8 f8 = new Funcion8();
	Funcion9 f9 = new Funcion9();
	
	public static void main(String[] args) {
		
		Menu();

	}
	
	public static void Menu() {
		Scanner sc = new Scanner(System.in);
		System.out.println("--- Menu ---\n"+
						   "1. Secuencial\n"+
						   "2. Condicional if\n"+
						   "3. Condicional 2.0\n"+
						   "4. Condicional Switch\n"+
						   "5. While\n"+
						   "6. Do while\n"+
						   "7. For\n"+
						   "8. Arreglo simple\n"+
						   "9. Arreglo Bidimensional\n"+
						   "10. Salir\n"
				);
		int sel = sc.nextInt();
		Seleccion(sel);
		sc.close();
	}
	
	public static void Seleccion(int sel) {
		
		switch(sel) {
			case 1:
				f1.Promedio();
			
			default:
				break;
		}
		
	}

}
